package ex01;

import java.util.Scanner;

public class ex10 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int ini, inj, x, y;
		Scanner scan = new Scanner(System.in);
		
		System.out.printf("За: ");
		ini = scan.nextInt();
		System.out.printf("ї­: ");
		inj = scan.nextInt();
		
		for (x = 0; x < ini; x++) {
			for (y = 0; y < inj; y++) {
				System.out.printf("[%d][%d] ", x,y);
			}
			System.out.println();
		}
	}

}
