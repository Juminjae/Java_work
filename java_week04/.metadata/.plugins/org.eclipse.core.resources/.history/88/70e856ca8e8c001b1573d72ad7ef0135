package ex01;

public class ex05 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int r = 5;
		double area = 0.0;
		
		area = r*r*3.14;
		
		System.out.println("반지름 "+ r +"를 가지는 원의 면적은 " + area + "입니다." );
		
	}

}
